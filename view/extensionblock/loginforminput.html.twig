<div class="owl">
	<div class="hand"></div>
	<div class="hand hand-r"></div>
	<div class="arms">
		<div class="arm"></div>
		<div class="arm arm-r"></div>
	</div>
</div>
<div class="form">
	<div class="control">
		<label for="user"  class="fa fa-envelope">{{ 'UI:Login:UserNamePrompt'|dict_s }}</label>
		<input id="user" type="text" name="auth_user" tabindex="1" value="{{ aData.sAuthUser }}" />
	</div>
	<div class="control">
			<label for="pwd"  class="fa fa-asterisk">{{ 'UI:Login:PasswordPrompt'|dict_s }}</label>
			<input id="pwd" type="password" name="auth_pwd" tabindex="2" autocomplete="off" value="{{ aData.sAuthPwd }}" />
	</div>
</div>

